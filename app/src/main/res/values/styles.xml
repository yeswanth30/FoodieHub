<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>
    </style>


    <style name="SlideInFromLeftDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_left</item>
    </style>

    <style name="SlideInDiagonalDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_in_diagonal</item>
    </style>

    <style name="SweetAlertDialog.Button.Pink" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/button_pink_background</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="TransparentButtonStyle" parent="@android:style/Widget.Button">
        <item name="android:textColor">@android:color/transparent</item>
    </style>


    <style name="DialogAnimation1">
        <item name="android:windowEnterAnimation">@anim/dialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit_anim</item>
    </style>


</resources>